{
  "name": "predicate-dispatch",
  "version": "1.0.2",
  "description": "Promise-accepting implementation of predicate dispatch",
  "main": "./lib",
  "scripts": {
    "preversion": "npm-run-all test:*",
    "docs": "jsdoc ./lib/*.js -R ./README.md -d ./docs",
    "test": "npm-run-all test:*",
    "test:lint": "eslint ./lib/*.js ./tests/*.js",
    "test:mocha": "mocha ./tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imbcmdth/predicate-dispatch.git"
  },
  "author": "Jon-Carlos Rivera",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "jsdoc": "^3.5.5",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
